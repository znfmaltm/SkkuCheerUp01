<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.kgh.skkucheerup.BookmarkActivity">

    <LinearLayout
        android:id="@+id/container4"
        android:layout_width="match_parent"
        android:layout_height="476dp"
        android:elevation="12dp"
        android:orientation="horizontal"
        android:visibility="visible">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/page4"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:layout_gravity="left"
        android:background="#cc000000"
        android:elevation="13dp"
        android:orientation="vertical"
        android:visibility="gone">

        <Button
            android:id="@+id/buttonMain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onmain1Clicked"
            android:text="메인페이지" />

        <Button
            android:id="@+id/buttonPast"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onPastClicked"
            android:text="지난 채용공고" />

        <Button
            android:id="@+id/buttonFav"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="즐겨찾기 목록" />

        <Button
            android:id="@+id/buttonMy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onSettingClicked"
            android:text="마이페이지" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="49dp"
            android:gravity="center_vertical|center_horizontal"
            android:text="즐겨찾기 목록"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView">

            <ImageButton
                android:id="@+id/zagwaBtn"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_marginEnd="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginRight="0dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="0dp"
                android:adjustViewBounds="true"
                android:contextClickable="true"
                android:elevation="0dp"
                android:onClick="onzagwaButtonClicked"
                android:padding="0dp"
                android:scaleType="fitXY"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView"
                app:srcCompat="@drawable/zagwa" />

            <ImageButton
                android:id="@+id/insaBtn"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_margin="0dp"
                android:layout_marginEnd="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginRight="0dp"
                android:layout_marginTop="0dp"
                android:adjustViewBounds="true"
                android:contextClickable="true"
                android:onClick="onInsaButtonClicked"
                android:padding="0dp"
                android:scaleType="centerInside"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/zagwaBtn"
                app:layout_constraintTop_toBottomOf="@+id/textView"
                app:srcCompat="@drawable/insa" />

        </LinearLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:elevation="9dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible"
                tools:context="org.kgh.skkucheerup.MainActivity">

                <LinearLayout
                    android:id="@+id/zagwaL4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="0dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:elevation="0dp"
                    android:orientation="vertical"
                    android:visibility="invisible"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout">

                    <ListView
                        android:id="@+id/listView34"
                        android:layout_width="match_parent"
                        android:layout_height="390dp"
                        android:elevation="5dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/insaL4"
                    android:layout_width="match_parent"
                    android:layout_height="390dp"
                    android:orientation="vertical">

                    <ListView
                        android:id="@+id/listView234"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </LinearLayout>

            </FrameLayout>
        </ScrollView>

    </LinearLayout>
</FrameLayout>
