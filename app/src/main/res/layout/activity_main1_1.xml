            android:text="지난 채용공고" />

        <Button
            android:id="@+id/buttonFav"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="즐겨찾기 목록" />

        <Button
            android:id="@+id/buttonMy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="마이페이지" />
